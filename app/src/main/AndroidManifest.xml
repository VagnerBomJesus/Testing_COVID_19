<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="pt.ipg.application.testingcovid_19">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TestActivity" />
        <activity
            android:name=".ui.login.DoctorLoginActivity"
            android:label="@string/title_activity_doctor_login" />
        <activity
            android:name=".ui.WelcomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.ForgotPasswordActivity" />
        <activity android:name=".ui.HomeActivity" />
        <activity
            android:name=".ui.WelcomeDoctorActivity"
            android:parentActivityName=".ui.WelcomeActivity" />
        <activity android:name=".ui.MainActivity" />
        <activity
            android:name=".ui.TermsServicesActivity"
            android:parentActivityName=".ui.WelcomeActivity" />
    </application>

</manifest>